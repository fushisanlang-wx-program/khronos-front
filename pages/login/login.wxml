<view class="weui-form">
  <view class="weui-form__text-area">
    <view class="weui-form__title">验明正身</view>
    <!--view class="weui-form__desc">表单描述示意</view-->
  </view>
  <view class="weui-form__control-area">
    <view class="weui-cells__group weui-cells__group_form">
        <!--view class="weui-cells__title">输入框</view-->
        <view class="weui-cells weui-cells_form">
          <view class="weui-cell">
         
            <view class="weui-cell__bd">
              <input placeholder='账号' value='{{username}}' bindinput='username' data-prop='username'></input>
         </view>
          </view>
          <view class="weui-cell">
            
            <view class="weui-cell__bd">
              <input placeholder='密码' type="password" value='{{password}}' bindinput='password' data-prop='password'></input>
            </view>
          </view>
        </view>
    </view>
  </view>


  <view class="weui-form__opr-area" >
    <button bindtap="doLogin"  class="doLogin" type="primary">确定</button>
  </view>
  <view class="weui-form__tips-area">
    <view class="weui-form__tips">

      <button class="weui-btn" type="default" bindtap="registered">第一次来</button>

       
    </view>
  </view>
  <view class="weui-footer weui-footer_fixed-bottom">
    <view class="weui-footer__text" bindtap="Fu">联系开发者</view>
            <view class="weui-footer__text">Copyright © 2021-2022 fushisanlang.cn</view>
        </view>
    
</view>
